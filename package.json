  "keywords": [
    "Integration",
    "SDK",
    "Widget",
    "Salesbot",
    "Amo",
    "Messenger",
    "AmoCRM"
  ],
  "author": "Amo (https://amo.tm/)",
  "homepage": "https://amo.tm/",
  "files": [
    "dist",
    "src",
    "pure.js"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@rollup/plugin-replace": "^2.3.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.2",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3"
  }
