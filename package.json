{
  "name": "@amo-tm/salesbot-sdk",
  "version": "0.0.1",
  "description": "Javascript SDK for salesbot widgets",
  "main": "dist/salesbot-sdk.js",
  "module": "dist/salesbot-sdk.esm.js",
  "jsnext:main": "dist/salesbot-sdk.esm.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
    "lint": "eslint '{src,types}/**/*.{ts,js}' && yarn prettier-list-different",
